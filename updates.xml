<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Forked Jelly Browser -->
  <remove-project name="LineageOS/android_packages_apps_Jelly" />
  <project path="packages/apps/Jelly" name="lin14-mGoms/android_packages_apps_Jelly" />

  <!-- microG Build, prebuilt apks -->
  <project path="prebuilts/prebuiltapks"
           name="MSe1969/android_prebuilts_prebuiltapks"
           clone-depth="1" revision="master" />

  <!-- Linaro Cross-compiler -->
  <project path="prebuilts/gcc/linux-x86/arm/linaro-arm-eabi-6.4"
           name="cm13-microG/prebuilt_linaro_gcc_arm-eabi"
           clone-depth="1" revision="6.4.1" />

  <!-- Privacy friendly netmonitor -->
  <project path="external/secuso/privacy-friendly-netmonitor"
           name="MSe1969/privacy-friendly-netmonitor-prebuilt"
           revision="master" />

  <!-- Patches & root-scripts Directory -->
  <project path="z_patches" name="lin14-mGoms/z_patches" revision="cm-14.1" />
  <project path="z_patches/root-scripts" name="lin14-mGoms/root-scripts" revision="cm-14.1" />

  <!-- CA Certificates from AOSP master branch -->
  <remove-project name="platform/system/ca-certificates" />
  <project name="platform/system/ca-certificates" path="system/ca-certificates" remote="aosp" revision="master" />

  <!-- LineageOS Android System Webview from 'lineage-15.1' branch -->
  <remove-project name="LineageOS/android_external_chromium-webview" />
  <project name="LineageOS/android_external_chromium-webview" 
           path="external/chromium-webview" clone-depth="1" revision="lineage-15.1" />

  <!-- Replaced/enhanced Repositories -->
  <remove-project name="LineageOS/android_external_sqlite" />
  <project path="external/sqlite" name="lin14-mGoms/android_external_sqlite" />  
  <remove-project name="LineageOS/android_frameworks_base" />
  <project path="frameworks/base" name="lin14-mGoms/android_frameworks_base" />  
  <remove-project name="LineageOS/android_frameworks_native" />
  <project path="frameworks/native" name="lin14-mGoms/android_frameworks_native" />  
  <remove-project name="LineageOS/android_packages_apps_Settings" />
  <project path="packages/apps/Settings" name="lin14-mGoms/android_packages_apps_Settings" />  
  <remove-project name="LineageOS/android_packages_apps_SetupWizard" />
  <project path="packages/apps/SetupWizard" name="lin14-mGoms/android_packages_apps_SetupWizard" />  
  <remove-project name="LineageOS/android_system_sepolicy" />
  <project path="system/sepolicy" name="lin14-mGoms/android_system_sepolicy" />  
  <remove-project name="LineageOS/android_vendor_cm" />
  <project path="vendor/cm" name="lin14-mGoms/android_vendor_cm" />  
  
</manifest> 

