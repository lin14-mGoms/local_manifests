  <remove-project name="LineageOS/android_frameworks_base" />
  <project path="frameworks/base" name="cm13-microG/android_frameworks_base" revision="cm-14.1-microG" />  
  <remove-project name="LineageOS/android_frameworks_native" />
  <project path="frameworks/native" name="cm13-microG/android_frameworks_native" revision="cm-14.1-microG" />  
  <remove-project name="LineageOS/android_packages_apps_Settings" />
  <project path="packages/apps/Settings" name="cm13-microG/android_packages_apps_Settings" revision="cm-14.1-microG" />  
  <remove-project name="LineageOS/android_packages_apps_SetupWizard" />
